{
  "name": "bunny-mark",
  "version": "1.1.0",
  "private": true,
  "description": "Simulation for testing Pixi.js performance",
  "scripts": {
    "test": "npm run build",
    "lint": "eslint src",
    "clean": "rimraf deploy/index.js",
    "prebuild": "npm run lint && npm run clean",
    "build": "browserify src/index.js -o deploy/index.js",
